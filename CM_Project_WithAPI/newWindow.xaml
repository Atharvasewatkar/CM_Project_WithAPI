<Window x:Class="CM_Project_WithAPI.newWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CM_Project_WithAPI"
        mc:Ignorable="d"
        Title="newWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="package" HorizontalAlignment="Left" Margin="422,133,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="price" HorizontalAlignment="Left" Margin="422,181,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="package" HorizontalAlignment="Left" Margin="332,124,0,0" VerticalAlignment="Top"/>
        <Label Content="price" HorizontalAlignment="Left" Margin="332,181,0,0" VerticalAlignment="Top"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="469,269,0,0" VerticalAlignment="Top" Click="AddPackage"/>
        <Label x:Name="reply" HorizontalAlignment="Left" Margin="444,217,0,0" VerticalAlignment="Top" Width="183"/>

    </Grid>
</Window>
